<?xml version="1.0" encoding="utf-8" ?>

<Patch>

	<!-- Veggie Blend -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
				<xpath>*/RecipeDef[defName = "VeggieMix5"]</xpath>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/RecipeDef[defName = "VeggieMix5"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>FSFButcher</requiredGiverWorkType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/RecipeDef[defName = "VeggieMixTwenty"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>FSFButcher</requiredGiverWorkType>
				</value>
			</li>
		</operations>
	</Operation>
	
</Patch>