<?xml version="1.0" encoding="utf-8" ?>

<Patch>

	<!-- Medieval Overhaul -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "DankPyon_DoBillsFurnace"]/workType</xpath>
				<value>
					<workType>FSFSmelt</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "DankPyon_DoBillsButcherFlesh"]/workType</xpath>
				<value>
					<workType>FSFButcher</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "DankPyon_DoBillsStonecut"]/workType</xpath>
				<value>
					<workType>FSFStoneCut</workType>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>