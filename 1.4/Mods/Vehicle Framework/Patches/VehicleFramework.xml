<?xml version="1.0" encoding="utf-8" ?>

<Patch>

	<!-- Vehicle Framework -->

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "PackVehicle"]/workType</xpath>
				<value>
					<workType>FSFLoading</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "PackVehicleTurret"]/workType</xpath>
				<value>
					<workType>FSFLoading</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "LoadUpgradeMaterials"]/workType</xpath>
				<value>
					<workType>FSFLoading</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "RefuelVehicle"]/workType</xpath>
				<value>
					<workType>FSFLoading</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "RepairVehicle"]/workType</xpath>
				<value>
					<workType>FSFRepair</workType>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGiverDef[defName = "DisassembleVehicle"]/workType</xpath>
				<value>
					<workType>FSFDeconstruct</workType>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>